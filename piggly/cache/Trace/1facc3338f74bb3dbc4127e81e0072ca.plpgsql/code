DECLARE
    res boolean;
BEGIN
    perform public.piggly_branch($PIGGLY$27227eb87f8bc758$PIGGLY$);
    SELECT x.indisunique
      FROM pg_catalog.pg_index x
      JOIN pg_catalog.pg_class ct    ON ct.oid = x.indrelid
      JOIN pg_catalog.pg_class ci    ON ci.oid = x.indexrelid
      JOIN pg_catalog.pg_namespace n ON n.oid = ct.relnamespace
     WHERE ct.relname = $2
       AND ci.relname = $3
       AND n.nspname  = $1
      INTO res;

      perform public.piggly_branch($PIGGLY$3744a6f4fe9a171d$PIGGLY$);RETURN ok( COALESCE(res, false), $4 );
END;