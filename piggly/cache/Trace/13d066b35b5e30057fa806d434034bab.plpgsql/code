DECLARE
    have REFCURSOR;
    want REFCURSOR;
    res  TEXT;
BEGIN
    perform public.piggly_branch($PIGGLY$ceb46591aafadeb5$PIGGLY$);
    OPEN have FOR EXECUTE _query($1);
    OPEN want FOR EXECUTE _query($2);
    res := results_eq(have, want, $3);
    CLOSE have;
    CLOSE want;
    perform public.piggly_branch($PIGGLY$fc27aa58c7981939$PIGGLY$);RETURN res;
END;